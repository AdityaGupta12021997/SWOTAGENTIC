name: My Workflow

on:
  push:
    branches:
      - main

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout code
        uses: actions/checkout@v3

      - name: Run Python script
        env:
          GOOGLE_API_KEY: ${{secrets.GOOGLE_API_KEY}}
        run: |
          python FT251006_Assignment2.py
